<h1 class="pt-3">Pokemones</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col-2">Nombre</th>
      <th scope="col-4">URL</th>
    </tr>
  </thead>
  <tbody>
    <% @response.each do |response| %>
      <tr>
        <td><%= response["name"] %></td>
        <td><%= response["url"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>